                  "Equipment": {
                    "type": "string"
                  },
                  "Goal": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "Trainer_Id": {
                  "style": "form"
                },
                "Title": {
                  "style": "form"
                },
                "Description": {
                  "style": "form"
                },
                "Price": {
                  "style": "form"
