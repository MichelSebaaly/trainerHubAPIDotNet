                "type": "object",
                "properties": {
                  "Trainer_Id": {
                    "type": "integer",
                    "format": "int32"
                  },
                  "Title": {
                    "type": "string"
                  },
                  "Description": {
                    "type": "string"
                  },
                  "Price": {
                    "type": "number",
                    "format": "double"
                  },
                  "File_URL": {
                    "type": "string",
